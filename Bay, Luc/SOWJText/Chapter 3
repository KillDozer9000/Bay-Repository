1.B. Public class Pet()
	 {
       Private String animal;
       Private String name;
       Private int age;

       Public Pet(String anim, String na, String ag)
	   {
         Animal = anim;
         Name = na;
         Age = ag;
       }
       Public getinsertvariablenamehere()
       {
         Return insertvariablenamehere;
       }
       Public String setname/animal(String a)
       {
         name/animal = a;
         Return name/animal;
       }
       Public String setage(int ag)
       {
         Age = ag;
         Return age;
       }

}
